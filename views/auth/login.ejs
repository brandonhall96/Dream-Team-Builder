<div class="signupdiv">
  <form class="signup" action="/auth/login" method="POST">
    <label for="auth-email">Email</label>
    <input class="inp 1" required id="auth-email" type="email" name="email">

    <label for="auth-password">Password</label>
    <input class="inp 2" required id="auth-password" type="password" name="password">

    <input class="supbutton" type="submit" value="Log In">
  </form>
</div>