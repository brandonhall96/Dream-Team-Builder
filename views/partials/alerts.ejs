<% if (alerts.error) { %>
    <% alerts.error.forEach(msg => { %>
     <div class="alert alert-danger"><%= msg %></div>
    <% }) %>
<% } %>

<% if (alerts.success) { %>
    <% alerts.success.forEach(msg => { %>
     <div class="alertsuccess">
<span class="close" onclick="this.parentElement.style.display='none';">&times;</span>
<%= msg %>

        </div>
    <% }) %>
<% } %>